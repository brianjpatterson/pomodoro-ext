{
    "manifest_version": 3,
    "name": "Pomodoro",
    "version": "1.0",
    "description": "A Pomodoro productivity extension",
    "icons": {
        "16": "images/pomo-16.png",
        "48": "images/pomo-48.png",
        "128": "images/pomo-128.png"
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
      ],
      "permissions": [
        "tabGroups"
      ],
    "action": {
        "default_popup": "popup/popup.html"
    }


}